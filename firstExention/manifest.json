{
  "manifest_version": 2,

  "name": "Copy History",
  "description": "Creates a history of text you have copied",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
